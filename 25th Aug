// input = [ "Bangalore is a city" ]
// output = [ "erolagnaB si a ytic" ]

// input = S74G5TY
// output = SGTY



// output = [ [ "eat", "tea", "ate"], [ "tan", "nat"], ["bat"] ]

function groupAnagram(strs)
{
    let anagramObj = {}

    // for of
    for(let str of strs)
    {
        // "eat".split() ==> [ "e", "a", "t" ] ==> [ "a", "e", "t" ]
        // [ "a", "e", "t" ] ==> "aet"

        // [ "eat", "tea", "tan", "ate", "nat", "bat" ]
        let sortedStr = str.split("").sort().join("")
        // sortedStr = "aet"
        
        if(!anagramObj[sortedStr])
        {
            anagramObj[sortedStr] = [str]
            // aet: [eat, tea, ate]
            ant: [tan]
        }
        else
        {
            anagramObj[sortedStr].push(str)
        }
        
        
        // anagramObj[aet] = [eat]
        // anagramObj = { aet: [eat, tea]}
        
    }

    return Object.values(anagramObj)
    
}

const result = groupAnagram([ "eat", "tea", "tan", "ate", "nat", "bat" ])
console.log(result)
