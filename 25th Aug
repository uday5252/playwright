// // Take an array that contains the salaries of an employee=,
// // and return the new salary with 5000 bonus for each employee

// // whenever we have a requirement to perform same opertaion
// // on each and every element present in the array ==> forEach()

// // const salaries = [ 45000, 32000, 50000, 60000, 15000 ]

// // salaries.forEach((a, b, c)=>
// // {
// //     console.log(a)
// //     console.log(b)
// //     console.log(c)
// // })

// // map()
// // Map() and forEach() 99.99% same

// // map() ==> rule
// // input elements count == output elements count
// // const salaries = [ 45000, 32000, 50000, 60000, 15000 ]

// // const result = salaries.map((a, b, c)=>
// // {
// //    if(a > 47000)
// //    {
// //         return a + 5000
// //    }
// // })

// // console.log(result)

// // const salaries = [ 45000, 32000, 50000, 60000, 15000 ]

// //  salaries.forEach((a, b, c)=>
// // {
// //     console.log(a + 5000)
// // })


// // const salaries = [ 45000, 32000, 50000, 60000, 15000 ]

// // const result = salaries.filter((a, b, c)=>
// // {
// //    if(a > 47000)
// //    {
// //         return a + 5000
// //    }
// // })

// // console.log(result)

// // map() will maintain input and output array elements
// // count to be same and it can perform operation
// // on each element

// // filter() will maintain input and output array elements
// // count to be different OR can be different



