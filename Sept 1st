
// local variables(a, b, c)
// Intsanc Variabkes(usermae, age, city)
const Student = function(username, age, city)
{
    this.username = username
    this.age = age
    this.city = city
}

Student.prototype.displayDetails = function()
    {
        console.log(this.username, this.age, this.city)
    }

// create 3 objects


const student1 = new Student("Raju", 10, "Pune", )
const student2 = new Student("Mary", 15, "Hyderabad")
const student3 = new Student("Cook", 12, "Bangalore")
const student4 = new Student("Ramu", 13, "Bangalore")

console.log(student1)
console.log(student2)
console.log(student3)
console.log(student4)

student1.displayDetails()
student2.displayDetails()
student3.displayDetails()
student4.displayDetails()


class Car
{
    static count = 0

    constructor()
    {
        Car.count++
    }

    static getCount()
    {
        console.log(Car.count)
    }
}

let k = 3
let parkingArr = [1, 4, 2]

for(let i = 0; i < k ; i++)
{
    for(let j = 0; j < parkingArr[i]; j++)
    {
        // i = 1 ==> second parking area
        // j = 0 ==> 3 < 4
        new Car()
        // j = 1 ==> terminate 
    }
    Car.getCount()
}



